<![CDATA[/*
   Microsoft SQL Server Integration Services Script Task
   Write scripts using Microsoft Visual C# 2008.
   The ScriptMain is the entry point class of the script.
*/

using System;
using System.Data;
using Microsoft.SqlServer.Dts.Runtime;
using System.Windows.Forms;
using System.Data.SqlClient;
using System.Data.Odbc;
using System.Text.RegularExpressions;



namespace ST_39117512fb8441a4b47c90714c3ebe10.csproj
{
    [System.AddIn.AddIn("ScriptMain", Version = "1.0", Publisher = "", Description = "")]
    public partial class ScriptMain : Microsoft.SqlServer.Dts.Tasks.ScriptTask.VSTARTScriptObjectModelBase
    {

        #region VSTA generated code
        enum ScriptResults
        {
            Success = Microsoft.SqlServer.Dts.Runtime.DTSExecResult.Success,
            Failure = Microsoft.SqlServer.Dts.Runtime.DTSExecResult.Failure
        };
        #endregion


        public void Main()
        {
            // d�claration et initialisation des variables

            string nomTable = Dts.Variables["vXmlNomTable"].Value.ToString();
            string columnsName = string.Empty;
            string columnsNameFinal = string.Empty;
            string MySqlQuery = string.Empty;
            
           

            OdbcConnection MysqlCon = new OdbcConnection();
            OdbcCommand MysqlCmd = new OdbcCommand();

            SqlConnection SqlCon;
            SqlCommand SqlCmd = new SqlCommand();

            // connection a Mysql 

            // d�claration et initialisation des variables de connexion MySql 
            //string mySqlDriver = "MySQL ODBC 5.1 Driver";
            string mySqlServeur = Dts.Variables["VidalCim_Server_Dest"].Value.ToString();
            //string mySqlPort = "3306";
            //string mySqlLogin = "root";
            //string mySqlPassword = "root";
            //string mySqlDataBase = Dts.Variables["VidalCim_DB_Dest"].Value.ToString(); 
            //string mySqlOption = "3";
            if (mySqlServeur == System.Net.Dns.GetHostName().ToString())
            {
                mySqlServeur = "localhost";
            }

            // d�claration et initialisation des variables de connexion SqlServer

            string sqlServerServer = Dts.Variables["RubisInfoCentreProduit_Server_Source"].Value.ToString();
            string sqlServerDataBase = Dts.Variables["RubisInfoCentreProduit_DB_Source"].Value.ToString();


            // chaine de connexion a Merlin Mysql
            //string MySqlConStr = "DRIVER=" + mySqlDriver + ";SERVER=" + mySqlServeur + ";PORT=" + mySqlPort + ";DATABASE=" + mySqlDataBase.ToString() + ";UID=" + mySqlLogin + ";PWD=" + mySqlPassword + ";OPTION=" + mySqlOption + "";
            string MySqlConStr = "Driver=Oracle ODBC Driver;" + "Data Source=RUBIS.FRANCE.GROUPE.INTRA;" + "Uid=sa;" + "Pwd=oracle;"; 
            MysqlCon = new OdbcConnection(MySqlConStr);

            // chaine de connexion a Merlin SqlServer
            string SqlConStr = "Server=" + sqlServerServer + ";Initial Catalog=" + sqlServerDataBase + ";Integrated Security=SSPI";
            SqlCon = new SqlConnection(SqlConStr);

            {

                // Construction de la requete pour la recuperation de donn�es dans SqlServer
                SqlCmd.CommandText = "SELECT * FROM " + nomTable.ToString() + ";";
                SqlCmd.Connection = SqlCon;

                try
                {
                    // ouverture de la connexion a SqlServer
                    SqlCon.Open();
                    SqlDataReader reader = SqlCmd.ExecuteReader();

                    // Boucle pour concatener le nom des colonnes et affectation de la varibale columnsNameFinal
                    
                    for (int i = 0; i < reader.FieldCount; i++)
                    {
                        int len = reader.GetName(i).ToString().Length;
                        
                            columnsName = reader.GetName(i).ToString();
                            
                            columnsNameFinal += columnsName + ",";
                        
                    }

                    // suppression de la derniere virgule 
                    columnsNameFinal = (columnsNameFinal.ToString().Substring(0, columnsNameFinal.Length - 1));

                    // ouverture de la connexion � Mysql
                    //MysqlCon.Open();

                    // Boucle sur les enregistrements
                    while (reader.Read())
                    {
                        // Declaraction et initialisation des variables pour stocker les valeurs retourn�es par SqlServer
                        string columnsValue = string.Empty;
                        string columnsValueFinal = string.Empty;

                        // Boucle pour chacune des colonnes de l'enregistrement courant 

                        for (int i = 0; i < reader.FieldCount; i++)
                        {
                            // recuperation du type de la colonne pour les transformations avant l insertion dans Mysql 
                            string columnsType = reader[i].GetType().Name.ToString();
                            
                            // Affichage du type de colonne 
                            // MessageBox.Show(columnsType);
                            // En Fonction du type blocant pour Mysql un traitement specifique permet de nettoyer la donn�e.

                            if (reader[i].ToString().Length >= 4000)
                            {
                                switch (columnsType)
                                {

                                    
                                    // les String sont avec une ' dans SQL, dans Mysql les ' doivent etre prefixe par \ pour etre inser�s
                                    case "String": columnsValue = ("'" + reader[i].ToString().Substring(0, 3999).Replace("'", "''") + "'"); break;
                                    default: columnsValue = reader[i].ToString(); break;

                                }

                            }
                            else
                            {
                                switch (columnsType)
                                {

                                    // les Booleens sont a True ou False dans SQL, dans Mysql ils doivent avoir la valeur 1 ou 0
                                    case "Boolean": columnsValue = reader[i].ToString().Replace("False", "0").Replace("True", "1"); break;
                                    // les D�cimaux sont avec un "," dans SQL, dans Mysql ils doivent avoir un "."
                                    case "Decimal": columnsValue = reader[i].ToString().Replace(",", ","); break;
                                    // les D�cimaux sont avec un "," dans SQL, dans Mysql ils doivent avoir un "."
                                    case "Float": columnsValue = "'" + reader[i].ToString().Replace(",", ",") + "'"; break;
                                    // les D�cimaux sont avec un "," dans SQL, dans Mysql ils doivent avoir un "."
                                    case "Single": columnsValue = "'" + reader[i].ToString().Replace(",", ",") + "'"; break;
                                    // les String sont avec une ' dans SQL, dans Mysql les ' doivent etre prefixe par \ pour etre inser�s
                                    case "String": columnsValue = ("TRIM('" + reader[i].ToString().Replace("'", "''") + "')"); break;
                                    // les DBNull sont avec une ' dans SQL, dans Mysql les ' doivent etre prefixe par \ pour etre inser�s
                                    case "DBNull": columnsValue = ("'" + reader[i].ToString().Replace("'", "''") + "'"); break;
                                    // les DBNull sont avec une ' dans SQL, dans Mysql les ' doivent etre prefixe par \ pour etre inser�s
                                    // case "DateTime": columnsValue = ("'" + reader[i].ToString() + "'"); break
                                    case "DateTime": columnsValue = ("TO_DATE('" + reader[i].ToString().Substring(0, 2) + "-" + reader[i].ToString().Substring(3, 2) + "-" + reader[i].ToString().Substring(6, 4) + reader[i].ToString().Substring(10, 9) + "','DD-MM-YYYY HH24:MI:SS')"); break;
                                    // les autres type de la base semblent pas poser de probleme
                                    default: columnsValue = reader[i].ToString(); break;

                                }
                            }
                            //MessageBox.Show(columnsValue);

                            // si une valeur est de longueur 0 alors elles sont remplac�es par NULL
                            //MessageBox.Show(reader.GetDataTypeName(i).ToString());
                            if (reader[i].ToString().Length == 0)
                            {
                                if (reader.GetDataTypeName(i).ToString() == "datetime")
                                {
                                    columnsValue = "Null";
                                }
                                if (reader.GetDataTypeName(i).ToString() == "int")
                                {
                                    columnsValue = "Null";
                                }
                                if (reader.GetDataTypeName(i).ToString() == "real")
                                {
                                    columnsValue = "Null";
                                }
                                if (reader.GetDataTypeName(i).ToString() == "float")
                                {
                                    columnsValue = "Null";
                                }
                               
                            }

                            // Concatenation de des valeurs a inserer pour la construction du Insert dans Mysql

                            columnsValueFinal += columnsValue + ","; 
                        }

                        // suppression de la derniere virgule pour l'insertion 
                        columnsValueFinal = columnsValueFinal.ToString().Substring(0, columnsValueFinal.Length - 1);

                        // génération de la requete d'insertion 
//                        MySqlQuery = "INSERT INTO " + nomTable + " (" + columnsNameFinal + ") values (" + columnsValueFinal + ")";
                        MySqlQuery = "INSERT INTO " + nomTable + " values (" + columnsValueFinal + "); ";
                        try
                        {

                            // test si la connection a mysq est ouverte
                            if (MysqlCon.State == ConnectionState.Closed)
                            {
                                MysqlCon.Open();
                                // MessageBox.Show(MySqlQuery);

                                // execution de la requete d'insert
                                MysqlCmd = new System.Data.Odbc.OdbcCommand(MySqlQuery, MysqlCon);
                                MysqlCmd.ExecuteNonQuery();
                                MysqlCon.Close();
                            }


                        }



                            // Catch de l'erreur lors de l insertion ou de la connection avec details
                        catch (System.Data.Odbc.OdbcException Ex)
                        {

                            MessageBox.Show(nomTable.ToString() + "\r\n" + MySqlQuery + "\r\n" + Ex.Message + "\r\n");
                            MessageBox.Show("Could not access the database.Please make sure you completed the fields with the correct information and try again. More ? ? ? details:" + Ex.Message, "Database connection error", MessageBoxButtons.OK, MessageBoxIcon.Error);
                            Dts.TaskResult = (int)ScriptResults.Failure;
                        }

                         }


                    reader.Close();

                }

                finally
                {
                    // fermeture de la connexion SQL
                    SqlCon.Close();
                    // fermeture de la connexion Mysql
                    MysqlCon.Close();

                }


            }
            // TODO: Add your code here
            Dts.TaskResult = (int)ScriptResults.Success;
        }
    }
